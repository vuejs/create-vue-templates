    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.46.0",
    "eslint-plugin-playwright": "^2.5.1",
    "eslint-plugin-vue": "~10.8.0",
    "globals": "^17.3.0",
    "jsdom": "^28.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.47.0",
    "prettier": "3.8.1",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.18"
